{
  "name": "@brendanle/awwwards-scrapers-scrapers",
  "version": "0.0.0",
  "description": "awwwards' web scrapers",
  "repository": {
    "type": "git",
    "url": "https://github.com/brendanle65/awwwards-scrapers.git",
    "directory": "packages/scrapers"
  },
  "author": "brendanle <brendan.mi.le@outlook.com>",
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "@comment scripts": {
    "build": "compile production code",
    "build:watch": "watch and compile code",
    "test": "test code",
    "test:watch": "watch and test code",
    "test:coverage": "collect tests' coverage",
    "clean": "remove compiled code and coverage code"
  },
  "scripts": {
    "build": "rimraf lib/ && tsc",
    "build:watch": "tsc --watch --sourcemap",
    "test": "yarn --cwd ../../ jest --selectProjects scrapers",
    "test:watch": "yarn --cwd ../../ jest --selectProjects scrapers --watch",
    "test:coverage": "yarn --cwd ../../ jest --selectProjects scrapers --coverage",
    "clean": "rimraf lib/ && rimraf coverage/"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "cheerio": "^1.0.0-rc.12"
  }
}
