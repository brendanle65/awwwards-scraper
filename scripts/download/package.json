{
  "name": "@brendanle/awwwards-scrapers-download",
  "version": "0.0.0",
  "description": "script to download scraped data to json files",
  "repository": {
    "type": "git",
    "url": "https://github.com/brendanle65/awwwards-scrapers.git",
    "directory": "scripts/download"
  },
  "author": "brendanle <brendan.mi.le@outlook.com>",
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "@comment scripts": {
    "build": "compile production code",
    "build:watch": "watch and compile code",
    "test": "test code",
    "test:watch": "watch and test code",
    "test:coverage": "collect tests' coverage",
    "clean": "remove compiled code and coverage code",
    "download:dir": "download directory of links data",
    "download:site": "download site awwward's info data",
    "download:user": "download user data of sites' creators"
  },
  "scripts": {
    "build": "rimraf build/ && tsc",
    "build:watch": "tsc --watch --sourcemap",
    "test": "yarn --cwd ../../ jest --selectProjects download",
    "test:watch": "yarn --cwd ../../ jest --selectProjects download --watch",
    "test:coverage": "yarn --cwd ../../ jest --selectProjects download --coverage",
    "clean": "rimraf build/ && rimraf coverage/",
    "download:dir": "node build/downloadDir.js",
    "download:site": "node build/downloadSite.js",
    "download:user": "node build/downloadUser.js"
  },
  "dependencies": {
    "@brendanle/awwwards-scrapers-utils": "0.0.0",
    "@brendanle/awwwards-scrapers-scrapers": "0.0.0"
  }
}
