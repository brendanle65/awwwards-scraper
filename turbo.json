{
  "baseBranch": "origin/production",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", "lib/**"],
      "cache": false
    },
    "build:watch": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", "lib/**"],
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
